                "bindings": [
                  {
                    "binding_name": "#network_world_player_count",
                    "binding_type": "collection",
                    "binding_collection_name": "$collection_name"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "padding_1": {
          "type": "panel",
          "size": [ 5, 0 ]
        }
      },
      {
        "padding_2": {
          "type": "panel",
          "size": [ 5, 32 ],
          "controls": [
            {
              "network_world_game_status_icon@play_old.network_world_game_status_icon": {
                "layer": 1
              }
            }
          ]
        }
      },
      {
        "padding_3": {
          "type": "panel",
          "size": [ 5, 0 ]
        }
      },
      {
        "network_world_type_icon@play.network_world_type_icon": {
          "layer": 4
        }
