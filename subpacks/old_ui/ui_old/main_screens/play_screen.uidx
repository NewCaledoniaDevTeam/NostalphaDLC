{
  "namespace": "play_old",

  /*///////////////////////////////////////////////////////////////////////////
  //  Grid.
  ///////////////////////////////////////////////////////////////////////////*/

  "world_item_grid_base": {
    "type": "grid",
    "size": [ "100%", "default" ],
    "layer": 1,
    "anchor_to": "top_left",
    "anchor_from": "top_left"
  },

  "realms_world_item_grid@play_old.world_item_grid_base": {
    "grid_item_template": "$grid_item_layout",
    "grid_dimension_binding": "$grid_name",
    "collection_name": "$realms_worlds_collection_name",
    "bindings": [
      {
        "binding_name": "$grid_name"
      },
      {
        "binding_name": "#realms_grids_visible",
        "binding_name_override": "#visible"
      }
    ]
  },

  "network_world_item_grid@play_old.world_item_grid_base": {
    "grid_item_template": "play_old.network_world_item",
    "grid_dimension_binding": "$network_world_item_grid_dimension",
    "collection_name": "$collection_name",
    "bindings": [
      {
        "binding_name": "$network_world_item_grid_dimension"
      }
    ]
  },

  "local_world_item_grid@play_old.world_item_grid_base": {
    "grid_item_template": "play_old.local_world_item",
    "grid_dimension_binding": "#local_world_item_grid_dimension",
    "collection_name": "local_worlds",
    "bindings": [
      {
        "binding_name": "#local_world_item_grid_dimension"
      }
    ]
  },

  "legacy_world_item_grid@play_old.world_item_grid_base": {
    "grid_item_template": "play.legacy_world_item",
    "grid_dimension_binding": "#legacy_world_item_grid_dimension",
    "collection_name": "legacy_worlds",
    "bindings": [
      {
        "binding_name": "#legacy_world_item_grid_dimension"
      }
    ]
  },

  "third_party_featured_server_grid": {
    "type": "grid",
    "size": [ "100%", "default" ],
    "layer": 1,
    "grid_item_template": "play_old.featured_server_world_item",
    "grid_dimension_binding": "#third_party_featured_item_grid_dimension",
    "collection_name": "third_party_server_network_worlds",
    "$collection_name": "third_party_server_network_worlds",
    "anchor_to": "top_left",
    "anchor_from": "top_left",
    "bindings": [
      {
        "binding_name": "#third_party_featured_item_grid_dimension"
      },
      {
        "binding_name": "#featured_servers_visible",
        "binding_name_override": "#visible"
      }
    ]
  },

  "more_servers_grid": {
    "type": "grid",
    "size": [ "100%", "default" ],
    "layer": 1,
    "grid_item_template": "play_old.more_servers_world_item",
    "grid_dimension_binding": "#servers_network_world_item_grid_dimension",
    "collection_name": "servers_network_worlds",
    "$collection_name": "servers_network_worlds",
    "anchor_to": "top_left",
    "anchor_from": "top_left",
    "bindings": [
      {
        "binding_name": "#servers_network_world_item_grid_dimension"
      }
    ]
  },

  /*///////////////////////////////////////////////////////////////////////////
  //  Scroll Panes.
  ///////////////////////////////////////////////////////////////////////////*/

  "scroll_stack_panel_base": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ]
  },

  "worlds_stack_panel@play_old.scroll_stack_panel_base": {
    "controls": [
      {
        "realms_panel": {
          "type": "stack_panel",
          "size": [ "100%", "100%c" ],
          "bindings": [
            {
              "binding_name": "#personal_realms_grid_visible",
              "binding_name_override": "#visible"
            }
          ],
          "controls": [
            {
              "realms_label@play_old.base_label": {
                "size": [ "default", 12 ],
                "ignored": "$world_picker_screen",
                "text": "playscreen.realms",
                "layer": 10,
                "bindings": [
                  {
                    "binding_name": "#realm_label_visible",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            },
            {
              "personal_realms@play_old.realms_world_item_grid": {
                "ignored": "$world_picker_screen",
                "$grid_name": "#personal_realms_grid_dimension",
                "$realms_worlds_collection_name": "personal_realms",
                "$grid_item_layout": "play_old.realms_world_item"
              }
            }
          ]
        }
      },
      {
        "worlds_label@play_old.base_label": {
          "size": [ "default", 12 ],
          "text": "playscreen.worlds",
          "bindings": [
            {
              "binding_name": "#local_worlds_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "grid@play_old.local_world_item_grid": {}
      }
    ]
  },

  "friends_stack_panel@play_old.scroll_stack_panel_base": {
    "controls": [
      {
        "friends_realms_label@play_old.base_label": {
          "size": [ "default", 12 ],
          "text": "playscreen.joinableRealms",
          "bindings": [
            {
              "binding_name": "#friends_realms_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "friends_realms@play_old.realms_world_item_grid": {
          "$grid_name": "#friend_realms_grid_dimension",
          "$realms_worlds_collection_name": "friends_realms",
          "$grid_item_layout": "play_old.realms_world_item"
        }
      },
      {
        "friends_label@play_old.base_label": {
          "size": [ "default", 12 ],
          "text": "networkWorld.joinable_friends_label",
          "bindings": [
            {
              "binding_name": "#friends_grid_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "friends_grid@play_old.network_world_item_grid": {
          "$network_world_item_grid_dimension": "#friends_network_world_item_grid_dimension",
          "$collection_name": "friends_network_worlds",

          "$network_world_icon": "play_old.friends_server_icon",
          "$hide_network_icon":  "$hide_xbox_live_icon"
        }
      },
      {
        "no_friends_grid_message@play_old.base_label": {
          "size": [ "default", 12 ],
          "text": "#no_friends_grid_message_text",
          "variables": [
            {
              "requires": "$nx_os",
              "$label_content_label": "networkWorld.joinable_nintendo_friends_label"
            }
          ],
          "bindings": [
            {
              "binding_name": "#no_friends_grid_message_text"
            },
            {
              "binding_name": "#no_friends_grid_message_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "lan_label@play_old.base_label": {
          "size": [ "default", 12 ],
          "text": "networkWorld.lan_label",
          "bindings": [
            {
              "binding_name": "#lan_grid_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "lan_grid@play_old.network_world_item_grid": {
          "$network_world_item_grid_dimension": "#lan_network_world_item_grid_dimension",
          "$network_world_icon": "play_old.lan_icon",
          "$collection_name": "lan_network_worlds"
        }
      }
    ]
  },

  "lan_icon": {
    "type": "image",
    "texture": "textures/ui_old/icon_lan",
    "size": [ 16, 16 ]
  },

  "friends_server_icon": {
    "type": "image",
    "size": [ 16, 16 ],
    "texture": "textures/ui_old/icon_xbox"
  },

  "server_stack_panel@play_old.scroll_stack_panel_base": {
    "controls": [
      {
        "more_servers_label@play_old.base_label": {
          "size": [ "default", 12 ],
          "text": "thirdPartyWorld.Featured",
          "layer": 1,
          "bindings": [
            {
              "binding_name": "#featured_servers_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "third_party_featured_grid@play_old.third_party_featured_server_grid": { }
      },
      {
        "more_servers_label@play_old.base_label": {
          "size": [ "default", 12 ],
          "text": "networkWorld.more_servers",
          "layer": 1,
          "bindings": [
            {
              "binding_name": "#servers_grid_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "more_servers_grid@play_old.more_servers_grid": { }
      }
    ]
  },

  /*///////////////////////////////////////////////////////////////////////////
  //  Button Panels.
  ///////////////////////////////////////////////////////////////////////////*/

  "local_world_item": {
    "type": "stack_panel",
    "size": [ "100%", 32 ],
    "orientation": "horizontal",
    "controls": [
      { "local_world_item_button@play_old.local_world_item_button": { } },
      { "local_world_edit_button@play_old.local_world_edit_button": { } }
    ]
  },

  "realms_world_item": {
    "type": "stack_panel",
    "size": [ "100%", 32 ],
    "orientation": "horizontal",
    "controls": [
      { "realms_world_item_button@play_old.realms_world_item_button": { } },
      {
        "edit_panel": {
          "type": "panel",
          "size": [ 32, 32 ],
          "controls": [
            { "realms_world_edit_button@play_old.realms_world_edit_button": { "layer": 1 } }
          ],
          "bindings": [
            {
              "binding_name": "#realms_edit_visible",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$realms_worlds_collection_name"
            }
          ]
        }
      },
      {
        "feed_panel": {
          "type": "panel",
          "size": [ 32, 32 ],
          "controls": [
            {
              "realms_feed_button@play_old.realms_feed_button": {
                "layer": 1
              }
            }
          ]
        }
      },
      {
        "leave_realm_panel": {
          "type": "panel",
          "size": [ 32, 32 ],
          "controls": [
            { "leave_friends_realm_button@play_old.leave_friends_realm_button": { "layer": 1 } }
          ],
          "bindings": [
            {
              "binding_name": "#leave_realm_button_visible",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$realms_worlds_collection_name"
            }
          ]
        }
      }
    ]
  },

  "network_world_item": {
    "type": "stack_panel",
    "size": [ "100%", 32 ],
    "orientation": "horizontal",
    "controls": [
      { "network_world_item_button@play_old.network_world_item_button": { } }
    ]
  },

  "featured_server_world_item": {
    "type": "stack_panel",
    "size": [ "100%", 32 ],
    "orientation": "horizontal",
    "controls": [
      { "more_servers_world_item_button@play_old.more_servers_world_item_button": { } }
    ]
  },

  "more_servers_world_item": {
    "type": "stack_panel",
    "size": [ "100%", 32 ],
    "orientation": "horizontal",
    "controls": [
      { "more_servers_world_item_button@play_old.more_servers_world_item_button": { } },
      { "network_server_world_edit_button@play_old.network_server_world_edit_button": { "ignored": "$ignore_add_servers" } }
    ]
  },

  /* Local World */

  "local_world_lock_panel": {
    "type": "panel",
    "size": [ "100%c", "100%c" ],
    "controls": [
      {
        "lock@ui_old_textures.icon_lock": {
          "color": "$text_color",
          "offset": [ 0, 1 ]
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "#world_lock_visible",
        "binding_type": "collection",
        "binding_name_override": "#visible",
        "binding_collection_name": "local_worlds"
      }
    ]
  },

  "local_world_text_panel": {
    "type": "panel",
    "size": [ "100%-10px", "100%-8px" ],
    "controls": [
      {
        "top_text": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", "50%" ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "controls": [
            {
              "local_world_name@play.local_world_name": {
                "shadow": true,
                "size": [ "fill", 9 ],
                "color": "$text_color",
                "layer": 1
              }
            },
            {
              "lock_1@play_old.local_world_lock_panel": {}
            },
            {
              "local_world_filesize@play.local_world_filesize": {
                "shadow": true,
                "size": [ "default", 9 ],
                "color": [ 0.75, 0.75, 0.75 ],
                "layer": 1
              }
            }
          ]
        }
      },
      {
        "bottom_text": {
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", "50%" ],
          "controls": [
            {
              "local_world_game_mode@play.local_world_game_mode": {
                "shadow": true,
                "size": [ "default", 9 ],
                "offset": [ 0, -1 ],
                "color": [ 0.75, 0.75, 0.75 ],
                "layer": 1
              }
            },
            {
              "padding_0": {
                "type": "panel",
                "size": [ 11, 0 ]
              }
            },
            {
              "local_world_date@play.local_world_date": {
                "shadow": true,
                "size": [ "default", 9 ],
                "offset": [ 0, -1 ],
                "color": [ 0.75, 0.75, 0.75 ],
                "layer": 1
              }
            }
          ]
        }
      }
    ]
  },

  /* Realms */

  "realms_world_item_button_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%", "100%" ],
    "layer": 4,
    "controls": [
      {
        "left_panel": {
          "type": "panel",
          "size": [ "fill", 32 ],
          "controls": [
            { "realms_text_panel@play_old.realms_text_panel": { } }
          ]
        }
      },
      {
        "padding_2": {
          "type": "panel",
          "size": [ "100%c", 32 ],
          "controls": [
            {
              "realms_world_player_count@play.world_list_text": {
                "text": "#realms_world_player_count",
                "color": "$secondary_text_color",
                "size": [ "default", "default" ],
                "shadow": true,
                "bindings": [
                  {
                    "binding_name": "#realms_world_player_count",
                    "binding_type": "collection",
                    "binding_collection_name": "$realms_worlds_collection_name"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "padding_1": {
          "type": "panel",
          "size": [ 5, 0 ]
        }
      },
      {
        "padding_2": {
          "type": "panel",
          "size": [ 5, 32 ],
          "controls": [
            { "realms_world_game_status_icon@play_old.realms_world_game_status_icon": {} }
          ]
        }
      },
      {
        "padding_3": {
          "type": "panel",
          "size": [ 5, 0 ]
        }
      }
    ]
  },

  "realms_world_game_status_icon": {
    "type": "panel",
    "size": [ 9, 10 ],
    "controls": [
      {
        "game_online_icon@ui_old_textures.dot_1": {
          "bindings": [
            {
              "binding_name": "#realms_game_online",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$realms_worlds_collection_name"
            }
          ]
        }
      },
      {
        "game_unavailable_icon@ui_old_textures.dot_2": {
          "bindings": [
            {
              "binding_name": "#realms_game_unavailable",
              "binding_name_override": "#visible",
              "binding_type": "collection",
              "binding_collection_name": "$realms_worlds_collection_name"
            }
