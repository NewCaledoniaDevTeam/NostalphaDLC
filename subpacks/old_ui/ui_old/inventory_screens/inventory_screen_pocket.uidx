  "crafting_and_search_icon": {
    "type": "panel",
    "size": [ 16, 16 ],
    "controls": [
      {
        "crafting": {
          "type": "panel",
          "ignored": "(not $is_crafting)",
          "controls": [
            { "crafting_icon@inventory_pocket_old.crafting_icon": { } }
          ]
        }
      },
      {
        "no_crafting": {
          "type": "panel",
          "ignored": "$is_crafting",
          "controls": [
            {
              "crafting_icon@inventory_pocket_old.crafting_icon": {
                "bindings": [
                  {
                    "binding_type": "global",
                    "binding_name": "(not #is_creative_mode)",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            },
            {
              "search_icon@inventory_pocket_old.search_icon": {
                "bindings": [
                  {
                    "binding_type": "global",
                    "binding_name": "#is_creative_mode",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  /*///////////////////////////////////////////////////////////////////////////
  //  Tab.
