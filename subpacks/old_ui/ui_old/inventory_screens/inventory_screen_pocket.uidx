  /* Left Tab */

  "left_tab@common_toggles_dx.toggle_panel": {
    "$unchecked_control": "ui_old_textures.TabLeftBack",
    "$checked_control": "ui_old_textures.TabLeftFront",
    "$unchecked_hover_control": "ui_old_textures.TabLeftBack",
    "$checked_hover_control": "ui_old_textures.TabLeftFront",

    "$toggle_focus_enabled": false,
    "$radio_toggle_group": true,
    "$toggle_name": "navigation_tab",
    "$toggle_group_default_selected": "$construction_index",
    "$tab_content": "crafting_pocket.empty_tab_panel"
  },

  "left_armor_tab@inventory_pocket_old.left_tab": {
    "$tab_view_binding_name": "armor_left_tab_toggle",
    "$tab_icon": "inventory_pocket_old.armor_icon",
    "$toggle_group_forced_index": "$left_armor_index",

    "$toggle_binding_type": "global",
    "$toggle_state_binding_name": "#is_left_tab_armor"
  },

  "crafting_tab@inventory_pocket_old.left_tab": {
    "$tab_view_binding_name": "crafting_left_tab_toggle",
    "$tab_icon": "inventory_pocket_old.crafting_icon",
    "$toggle_group_forced_index": "$left_crafting_index",

    "$toggle_binding_type": "global",
    "$toggle_state_binding_name": "#is_left_tab_crafting"
  },

  "left_inventory_tab@inventory_pocket_old.left_tab": {
    "$unchecked_control": "ui_old_textures.TabLeftBack",
    "$checked_control": "ui_old_textures.TabLeftFront",
    "$unchecked_hover_control": "ui_old_textures.TabLeftBack",
    "$checked_hover_control": "ui_old_textures.TabLeftFront",

    "$tab_view_binding_name": "inventory_left_tab_toggle",
