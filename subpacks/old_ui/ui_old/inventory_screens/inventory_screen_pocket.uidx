  "tab_navigation_panel_layout": {
    "type": "panel",
    "layer": 4,
    "size": [ "100%+52px", "100%" ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "controls": [
      {
        "left_navigation_tabs": {
          "type": "panel",
          "size": [ 30, "100%" ],
          "anchor_to": "top_left",
          "anchor_from": "top_left",
          "controls": [
            {
              "content": {
                "type": "stack_panel",
                "size": [ "100%", "100%" ],
                "anchor_to": "top_left",
                "anchor_from": "top_left",
                "controls": [
                  {
                    "close_tab@ui_old_textures.TabLeftFrontTopMost": { 
                      "size": [ 30, 28 ],
                      "controls": [
                        { 
                          "default@common_old.close_button": {
                            "anchor_to": "top_left",
                            "anchor_from": "top_left",
                            "offset": [ 5, 6 ]
                          }   
                        }
                      ]
                    } 
									},
                  { "fill_panel_0": { "type": "panel", "size": [ 1, "fill" ] } },
									{ "left_armor_tab@inventory_pocket_old.left_armor_tab": { "size": [ 30, 28 ] } },
                  { "fill_panel_1": { "type": "panel", "size": [ 1, 2 ] } },
                  { "search_tab@inventory_pocket_old.search_tab_toggle": { "size": [ 30, 28 ] } },
                  { "fill_panel_2": { "type": "panel", "size": [ 1, 2 ] } },
                  { "left_inventory_tab@inventory_pocket_old.left_inventory_tab": { "size": [ 30, 28 ] } },
									{ "fill_panel_3": { "type": "panel", "size": [ 1, 2 ] } }
                ]
              }
            }
          ]
        }
      },
      {
        "right_navigation_tabs": {
          "type": "panel",
          "size": [ 30, "100%" ],
          "anchor_to": "top_right",
          "anchor_from": "top_right",
          "ignored": "$is_crafting",
          "controls": [
            {
              "content": {
                "type": "stack_panel",
                "size": [ "100%", "100%" ],
                "anchor_to": "top_right",
                "anchor_from": "top_right",
                "controls": [
                  { "fill_panel_0": { "type": "panel", "size": [ 1, "fill" ] } },
                  { "construction_tab@inventory_pocket_old.construction_tab_toggle": { "size": [ 30, 28 ] } },
                  { "fill_panel_1": { "type": "panel", "size": [ 1, 2 ] } },
                  { "equipment_tab@inventory_pocket_old.equipment_tab_toggle": { "size": [ 30, 28 ] } },
                  { "fill_panel_2": { "type": "panel", "size": [ 1, 2 ] } },
                  { "items_tab@inventory_pocket_old.items_tab_toggle": { "size": [ 30, 28 ] } },
                  { "fill_panel_3": { "type": "panel", "size": [ 1, 2 ] } },
                  { "nature_tab@inventory_pocket_old.nature_tab_toggle": { "size": [ 30, 28 ] } },
									{ "fill_panel_4": { "type": "panel", "size": [ 1, 2 ] } }
                ]
              }
            }
          ],
          "bindings": [
            {
              "binding_type": "global",
              "binding_name": "#is_creative_mode",
              "binding_name_override": "#visible"
            }
          ]
        }
      }
    ]
  },

